{
    "name": "encode",
    "title": "Encode Activity",
    "version": "1.0.0",
    "type": "flogo:activity",
    "author": "Dheeren Karam",
    "display": {
       "category": "Custom",
       "visible": true,
       "description": "This activity returns the sha256 of input string and salt"
    },
 
    "ref": "github.com/kdnreddy/flogo/encode",
    "inputs": [
           {
            "name": "inputString",
            "type": "string",
            "required": true
           },
           {
            "name": "salt",
            "type": "string",
            "required": true
           }
    ],
  
    "outputs": [
           {
            "name": "result",
            "type": "string"
          }
    ]
}
